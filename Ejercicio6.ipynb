      "source": [
        "def sumarSerieInversa(n: int, x: float) -> float:\n",
        "    suma = 0\n",
        "    for i in range(n, 0, -1):\n",
        "        suma += x ** i\n",
        "    return suma\n"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "\n",
        "n = 5\n",
        "x = 2\n",
        "resultado = sumarSerieInversa(n, x)\n",
        "print(\"La suma de la serie en orden inverso es:\", resultado)"
